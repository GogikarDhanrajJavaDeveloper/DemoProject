<?xml version="1.0" encoding="UTF-8"?>

<web-app>

<servlet>
<servlet-name>HomeServlet</servlet-name>
<servlet-class>com.dhanraj.HomeServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>HomeServlet</servlet-name>
<url-pattern>/home</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>AdminLogin</servlet-name>
<servlet-class>com.dhanraj.AdminLoginServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>AdminLogin</servlet-name>
<url-pattern>/admin</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>AddServiceServlet</servlet-name>
<servlet-class>com.dhanraj.AddServiceServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>AddServiceServlet</servlet-name>
<url-pattern>/addservice</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>ReteriveService</servlet-name>
<servlet-class>com.dhanraj.ReteriveServiceServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>ReteriveService</servlet-name>
<url-pattern>/view</url-pattern>
</servlet-mapping>

<servlet>
<servlet-name>LogOut</servlet-name>
<servlet-class>com.dhanraj.LogoutServlet</servlet-class>
</servlet>
<servlet-mapping>
<servlet-name>LogOut</servlet-name>
<url-pattern>/logout</url-pattern>
</servlet-mapping>

<welcome-file-list>
<welcome-file>home.html</welcome-file>
</welcome-file-list>
</web-app>